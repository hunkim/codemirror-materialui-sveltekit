{
  "name": "codemirror-matrialui-svelte-modules",
  "version": "0.0.0-development",
  "description": "Svelte wrapper for CodeMirror v6 and MatrialUI",
  "types": "./CodeMirror.svelte.d.ts",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@material/theme": "^13.0.0",
    "@material/typography": "^13.0.0",
    "@mdi/js": "^6.6.96",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/icon-button": "^6.0.0-beta.16",
    "@smui/top-app-bar": "^6.0.0-beta.16",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/codemirror": "^5.60.5",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "firebase": "^9.8.4",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "semantic-release": "^19.0.2",
    "smui-theme": "^5.0.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.9",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/state": "^0.20.0",
    "@codemirror/theme-one-dark": "^0.20.0",
    "@codemirror/view": "^0.20.4",
    "@microsoft/tsdoc": "^0.14.1",
    "@semantic-release/changelog": "^6.0.1",
    "codemirror": "^6.0.0",
    "@codemirror/commands":"^0.20.0",
    "@codemirror/fold": "^0.19.4",
    "@codemirror/highlight": "^0.20.0",
    "@codemirror/history": "^0.19.2",
    "@codemirror/lang-markdown": "^0.20.0",
    "@codemirror/lang-python": "^0.20.0",
    "@codemirror/lang-javascript": "^0.20.0",
    "@codemirror/legacy-modes": "^0.20.0",
    "@codemirror/search": "^0.20.0",
    "@codemirror/stream-parser": "^0.19.9"
  },
  "repository": "https://github.com/mtmeyer/codemirror-svelte-modules",
  "exports": {
    ".": "./CodeMirror.svelte"
  }
}
